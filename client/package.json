{
  "name": "meeting-scheduler-client",
  "private": true,
  "version": "0.1.1",
  "description": "This file boots a Vite-powered development server that serves the React client app at `http://localhost:5173`. It uses React Router for navigation, Apollo Client for GraphQL, and React Hook Form for form handling. The app is styled with Bootstrap and uses date-fns for date manipulation.",
  "engines": {
    "node": ">=22.14.0 <23.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "vite",
    "dev:hot": "vite --host",
    "build": "tsc -b && vite build",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist",
    "build:stats": "tsc -b && vite build --mode=stats",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "bundle:size": "npm run build && npx bundlesize",
    "performance:lighthouse": "npx lighthouse http://localhost:5173 --output=html --output-path=./lighthouse-report.html"
  },
  "dependencies": {
    "@apollo/client": "3.3.11",
    "@types/lodash": "^4.17.20",
    "bootstrap": "5.3.3",
    "crypto-hash": "^4.0.0",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.1",
    "filepond": "^4.32.9",
    "filepond-plugin-file-validate-size": "^2.2.8",
    "filepond-plugin-file-validate-type": "^1.2.9",
    "filepond-plugin-image-preview": "^4.6.12",
    "filepond-plugin-image-resize": "^2.0.10",
    "graphql": "^15.9.0",
    "lodash": "^4.17.21",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-filepond": "^7.1.3",
    "react-hook-form": "^7.52.1",
    "react-loading-skeleton": "^3.5.0",
    "react-router-dom": "^6.23.0",
    "react-select": "^5.10.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@graphql-typed-document-node/core": "^3.2.0",
    "@hookform/devtools": "^4.3.1",
    "@hookform/resolvers": "^3.9.0",
    "@types/bootstrap": "^5.2.10",
    "@types/node": "^20.14.10",
    "@types/react": "16.14.55",
    "@types/react-dom": "16.9.24",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "prettier": "^3.3.3",
    "sass": "^1.77.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.10"
  }
}
